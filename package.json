    "nodenv": "0.1.0",
    "pg": "8.11.3",
    "reflect-metadata": "0.2.2",
    "tsconfig-paths": "4.2.0",
    "typeorm": "0.3.17",
    "ts-node": "10.9.2",
    "debug": "4.4.3",
    "ms": "2.1.3"
  },
  "devDependencies": {
    "@types/node": "24.10.0",
    "nodemon": "3.0.1",
    "prettier": "3.6.2",
